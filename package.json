  "scripts": {
    "start": "NODE_ENV=development node run.js",
    "start:prod": "npm run build && cross-env NODE_ENV=production babel-node ./server.js && webpack -p",
    "start:test": "NODE_ENV=test node run.js",
    "start:proxy": "node proxy.js",
    "build": "webpack --config webpack.dev.js --progress --profile --colors",
    "postinstall": "webpack -p --config ./webpack.prod.js --env=prod --progress",
    "build:dist": "webpack --config webpack.prod.js --env=prod --progress --profile --colors",
    "build:test": "webpack --config webpack.test.js --env=test --progress --profile --colors",
    "deploy": "aws s3 sync --acl public-read --profile default --delete public/ s3://shoppylife.ml && npm run invalidate",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1KRF3QW81L9O --paths ''/*''",
    "s3-upload": "node s3upload.js",
    "publish:webapp": "node tools/build.js && node s3upload.js"
  },
